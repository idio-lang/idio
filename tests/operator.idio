test (10 + 9) 19
test (10 - 9) 1
test (10 + 9 + 1) 20
test (10 + 9 - 1) 18

test (2 + 3 * 4) 14
test (2 * 3 + 4) 10
test (2 + 3 * 4 + 5) 19
test (2 * 3 + 4 * 5) 26

test (1 and 2 and 3) 3
test (1 and #f and 3) #f
test (1 or 2 or 3) 1
test (#f or #f or 3) 3
test (1 and 2 or 3) 2
test (#f and 2 or 3) 3
test (1 or 2 and 3) 3
test (#f or 2 and 3) 3

test (1 and 2 and 3 and 4) 4
test (1 and 2 and 3 or 4) 3
test (1 or 2 or 3 and 4) 4

