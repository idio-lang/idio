test (s9/false #f 1 2 3) (void)

define-syntax fluid-let \
  (syntax-rules ()
    ((_ () expr & exprs)
     { expr & exprs })
    ((_ ((v1 a1) (v2 a2) ...) expr & exprs)
      (let ((outer-v v1))
        (set! v1 a1)
        (fluid-let ((v2 a2) ...)
          (let ((r (begin expr & exprs)))
            (set! v1 outer-v)
            r)))))

