define-syntax iff \
  (syntax-rules (then else)
    ((_ p then t)
      (and p t))
    ((_ p then t else f)
      (cond (p t) (else f))))
test (iff #t then 'foo) 'foo
test (iff #f then 'foo) #f
test (iff #t then 'foo else 'bar) 'foo
test (iff #f then 'foo else 'bar) 'bar
