test (bar-syntax x) '((x x))
test (bar-syntax x y) '((x x) (y y))
test (bar-syntax x y z) '((x x) (y y) (z z))

define-syntax rev-syntax \
	      (syntax-rules ()
			    ((_ #n r) r)
			    ((_ (a & d) r) (rev-syntax d (a & r)))
		            ((_ x) (rev-syntax x '())))
test (rev-syntax #n) '()
;test (rev-syntax (2 1 pair)) '(1 & 2)
test (rev-syntax ('bar 'foo #t if)) 'foo

