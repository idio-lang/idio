define (visibility-check x) {
  delete-file testfile

  out := open-output-file testfile
  write x out
  display #\{space} out
  display x out
  display #\{space} out
  write 'the-end out
  close-output-handle out

  in := open-input-file testfile
  vis := read-expr in
  invis := read-expr in
  close-input-handle in

  list vis invis
}

