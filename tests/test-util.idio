test (string #U+61) "#\\a"
test (format "%s" #U+61) "a"
test (format "%s" #U+A4) "Â¤"		; >= 0x80 will be two bytes - 00A4;CURRENCY SIGN
test (format "%s" #U+B87) "à®‡"		; >= 0x800 will be three bytes - 0B87;TAMIL LETTER I
